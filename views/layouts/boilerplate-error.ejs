        const validateForm = function () {
            let formToValidate = document.querySelector('.form-to-validate')
            if(!formToValidate.checkValidity()){
                console.log('Something is invalid')
            } else {
                submitFormData()
            }
            formToValidate.classList.add('was-validated')
        }
